
@{
    Layout = null;
    if(Session["userID"] == null)
    {
        Response.Redirect("~/Login/Index");
    }
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard</title>
</head>
<body>
    <div> 
        <h1>Dashboard</h1>
        <h3>Username: @Session["username"].ToString()</h3>
        <a href="@Url.Action("LogOut","Login")">LogOut</a>
    </div>
</body>
</html>
